<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zongshe.dao.ReportDao">
    <insert id="insertReport" useGeneratedKeys="true" keyProperty="id" keyColumn="id" parameterType="com.zongshe.pojo.Report">
        INSERT INTO
        jat_report(content,repTime)
        VALUES
        (#{content},#{repTime})
    </insert>
</mapper>